{
  "name": "nodejs-hometask",
  "version": "0.0.1",
  "description": "Hometask for NodeJS and express lecture",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "sh -c 'env REPO_URL=$0 npx mocha tests/**/*.spec.js -R JSON --exit'",
    "test-local": "./node_modules/.bin/mocha tests/**/*.spec.js --exit -R JSON",
    "build-front": "cd client | ls",
    "feedback": "node ./feedback/index.js",
    "generate": "npm run test-local | npm run feedback",

    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --config nodemon.json index.ts",
    "test2": "jest --config jest.config.json"
  },
  "keywords": [
    "NodeJS",
    "expressJS"
  ],
  "license": "MIT",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.18.2",
    "lodash.isempty": "4.4.0",
    "lowdb": "1.0.0",
    "request": "2.88.2",
    "uuid": "9.0.1"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
